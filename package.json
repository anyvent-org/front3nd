{
  "name": "@anyvent-org/front3nd",
  "version": "0.0.10",
  "description": "Various frontend related fragments used across anyvent.org projects",
  "main": "index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "front3nd",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "bin": {
    "front3nd": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "cd test && webpack --config webpack.config.js --mode production",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anyvent-org/front3nd.git"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/lib"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "frontend",
    "webpack",
    "scss",
    "toolbox"
  ],
  "author": "Matthias Rohmer <rohmer.matthias@anyvent.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anyvent-org/front3nd/issues"
  },
  "homepage": "https://github.com/anyvent-org/front3nd#readme",
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "globby": "11.0.4"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.2",
    "pkg-dir": "5.0.0",
    "css-loader": "5.2.6",
    "css-minimizer-webpack-plugin": "3.0.2",
    "eslint": "7.30.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "file-loader": "6.2.0",
    "filemanager-webpack-plugin": "6.1.4",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "mini-css-extract-plugin": "2.1.0",
    "mini-svg-data-uri": "1.3.3",
    "node-sass": "6.0.1",
    "oclif": "1.18.1",
    "prettier": "2.3.2",
    "sass-loader": "12.1.0",
    "style-loader": "3.0.0",
    "svg-spritemap-webpack-plugin": "4.1.0",
    "svgo": "2.3.1",
    "svgo-loader": "3.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.42.0",
    "webpack-build-notifier": "2.3.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  }
}
